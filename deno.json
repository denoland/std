{
  "name": "@dreamcatcher-ai/artifact",
  "version": "0.0.1",
  "lock": false,
  "tasks": {
    "test": "DENO_KV_PATH=:memory: deno test -A --coverage --clean --parallel --trace-leaks",
    "watch": "clear && DENO_KV_PATH=:memory: DEBUG_COLORS=1 deno test -A --watch",
    "api": "DENO_KV_PATH=:memory: DEBUG_COLORS=1 deno run -A --watch --inspect-wait main.ts",
    "check:types": "deno check **/*.ts",
    "check": "deno fmt --check && deno lint && deno task check:types",
    "ok": "deno task check && deno task test",
    "cov:gen": "deno coverage coverage --lcov --exclude='.tsx' --output=cov.lcov",
    "tc": "DEBUG_COLORS=1 deno test -A --unsafely-ignore-certificate-errors --trace-leaks ./server/cloud.ts",
    "db:drop": "DEBUG_COLORS=1 deno run -A ./server/db-drop.ts",
    "db:tools": "DEBUG_COLORS=1 deno run -A ./server/db-tools.ts",
    "keygen": "DEBUG_COLORS=1 deno run ./server/keygen.ts",
    "bw": "DENO_KV_PATH=:memory: DEBUG_COLORS=1 deno run -A --check --watch benchmarks/bench.ts",
    "bench": "DENO_KV_PATH=:memory: DEBUG_COLORS=1 deno run -A benchmarks/bench.ts"
  },
  "imports": {
    "$git": "npm:isomorphic-git",
    "@/": "./",
    "@deno/kv-oauth": "jsr:@deno/kv-oauth",
    "@io/": "./io/",
    "@kitsonk/kv-toolbox": "jsr:@kitsonk/kv-toolbox",
    "@noble/hashes": "npm:@noble/hashes",
    "@noble/secp256k1": "npm:@noble/secp256k1",
    "@scure/base": "npm:@scure/base",
    "@sindresorhus/is": "npm:@sindresorhus/is",
    "@std/assert": "jsr:@std/assert",
    "@std/async": "jsr:@std/async",
    "@std/dotenv": "jsr:@std/dotenv",
    "@std/expect": "jsr:@std/expect",
    "@std/path": "jsr:@std/path",
    "@types/benchmark": "npm:@types/benchmark",
    "@utils": "./utils.ts",
    "benchmark": "npm:benchmark",
    "chalk": "npm:chalk",
    "debug": "npm:debug",
    "diff": "npm:diff",
    "diff3": "npm:diff3",
    "eventsource-parser/stream": "npm:eventsource-parser/stream",
    "fast-deep-equal/es6": "npm:fast-deep-equal/es6/index.js",
    "gray-matter": "npm:gray-matter",
    "hono": "jsr:@hono/hono",
    "it-pushable": "npm:it-pushable",
    "lodash.merge": "npm:lodash.merge",
    "micromatch": "npm:micromatch",
    "multiformats": "npm:multiformats",
    "openai": "npm:openai",
    "serialize-error": "npm:serialize-error",
    "sharp": "npm:sharp",
    "supports-color": "npm:supports-color",
    "ulid": "npm:ulid",
    "youtube-caption-extractor": "npm:youtube-caption-extractor",
    "zod": "npm:zod",
    "zod-to-json-schema": "npm:zod-to-json-schema"
  },
  "exclude": ["cov/"],
  "lint": {
    "rules": {
      "tags": ["recommended"],
      "include": ["no-boolean-literal-for-arguments"]
    }
  },
  "scopes": {},
  "fmt": {
    "exclude": ["coverage", "cov", "node_modules", "HAL", "tmp"],
    "semiColons": false,
    "singleQuote": true,
    "proseWrap": "always",
    "indentWidth": 2,
    "useTabs": false
  },
  "unstable": ["kv", "fmt-html", "fmt-css", "fmt-component"],
  "exports": "./main.ts"
}
