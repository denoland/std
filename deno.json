{
  "lock": false,
  "tasks": {
    "init:stripe": "deno run --allow-read --allow-env --allow-net --env tasks/init_stripe.ts",
    "db:dump": "deno run --allow-read --allow-env --unstable tasks/db_dump.ts",
    "db:restore": "deno run --allow-read --allow-env --unstable tasks/db_restore.ts",
    "db:seed": "deno run --allow-read --allow-env --allow-net --unstable tasks/db_seed.ts",
    "db:migrate": "deno run --allow-read --allow-env --allow-net --unstable tasks/db_migrate.ts",
    "db:reset": "deno run --allow-read --allow-env --unstable tasks/db_reset.ts",
    "start": "deno run --unstable -A --watch=static/,routes/ --env dev.ts",
    "test": "DENO_KV_PATH=:memory: deno test -A --unstable-kv --coverage",
    "watch": "DENO_KV_PATH=:memory: DEBUG_COLORS=1 deno test -A --unstable-kv --watch",
    "api": "DENO_KV_PATH=:memory: DEBUG_COLORS=1 deno run --inspect-brk --unstable-kv -A --watch=artifact/ artifact/main.ts",
    "check:types": "deno check **/*.ts && deno check **/*.tsx",
    "ok": "deno fmt --check && deno lint && deno task check:types",
    "cov:gen": "deno coverage coverage --lcov --exclude='.tsx' --output=cov.lcov",
    "update": "deno run -A -r https://fresh.deno.dev/update .",
    "build": "deno run -A --unstable dev.ts build",
    "preview": "deno run -A --unstable main.ts",
    "esm:add": "deno run -A https://esm.sh/v135 add",
    "esm:update": "deno run -A https://esm.sh/v135 update",
    "esm:remove": "deno run -A https://esm.sh/v135 remove",
    "setup:githook": "ln -s ./pre-commit .git/hooks/pre-commit"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "imports": {
    "@/": "./",
    "@io/": "./artifact/io/",
    "$std/": "https://deno.land/std/",
    "std/": "https://deno.land/std/",
    "kv_oauth/": "https://deno.land/x/deno_kv_oauth@v0.9.1/",
    "$fresh/": "https://deno.land/x/fresh@1.6.3/",
    "fresh_charts/": "https://deno.land/x/fresh_charts@0.3.1/",
    "$gfm": "https://deno.land/x/gfm@0.2.5/mod.ts",
    "@utils": "./artifact/utils.ts",
    "tabler_icons_tsx/": "https://deno.land/x/tabler_icons_tsx@0.0.4/tsx/",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.0",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.1",
    "$git": "https://esm.sh/isomorphic-git",
    "$git/": "https://esm.sh/isomorphic-git/",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.2.2",
    "preact": "https://esm.sh/preact@10.19.2",
    "preact/": "https://esm.sh/preact@10.19.2/",
    "feed": "npm:/feed@4.2.2",
    "stripe": "npm:/stripe@13.5.0",
    "tailwindcss/": "npm:/tailwindcss@3.3.5/",
    "tailwindcss/plugin": "npm:/tailwindcss@3.3.5/plugin.js",
    "tailwindcss": "npm:tailwindcss@3.3.5"
  },
  "exclude": ["cov/", "_fresh/", "**/_fresh/*"],
  "lint": {
    "rules": {
      "tags": ["fresh", "recommended"]
    }
  },
  "scopes": {},
  "fmt": {
    "exclude": ["coverage", "cov", "_fresh", "node_modules"],
    "semiColons": false,
    "singleQuote": true,
    "proseWrap": "always"
  },
  "nodeModulesDir": true
}
